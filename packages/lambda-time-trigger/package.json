{
  "name": "paws-lambda-time-trigger",
  "version": "1.0.0",
  "description": "Implements sub-minute time trigger support to trigger AWS Lambda functions",
  "homepage": "https://github.com/ChristianEder/paws/packages/lambda-time-trigger",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "build": "jsii -vvv",
    "build:watch": "jsii -w",
    "package": "jsii-pacmak -vvv"
  },
  "keywords": [
    "jsii",
    "cdk",
    "aws",
    "lambda",
    "time trigger",
    "cron"
  ],
  "jsii": {
    "outdir": "dist",
    "targets": {
      "python": {
        "distName": "paws.lambda-time-trigger",
        "module": "paws.lambda-time-trigger"
      },
      "java": {
        "package": "paws.lambda.timetrigger",
        "maven": {
          "groupId": "com.paws",
          "artifactId": "lambda.timetrigger"
        }
      },
      "dotnet": {
        "namespace": "Paws.Lambda.TimeTrigger",
        "packageId": "Paws.Lambda.TimeTrigger"
      }
    }
  },
  "license": "Apache-2.0",
  "author": {
    "name": "Christian Eder",
    "email": "christian.johann.eder@googlemail.com"
  },
  "repository": {
    "url": "https://github.com/ChristianEder/paws.git"
  },
  "devDependencies": {
    "jsii": "1.52.1",
    "jsii-pacmak": "1.52.1",
    "aws-cdk-lib": "2.8.0",
    "constructs": "10.0.0"
  },
  "peerDependencies": {
    "aws-cdk-lib": "2.8.0",
    "constructs": "^10.0.0"
  },
  "dependencies": {
    "cron-parser": "4.2.1"
  },
  "bundledDependencies": [
    "cron-parser"
  ]
}